{
  "name": "telegram-intelligence-scraper",
  "version": "1.0.0",
  "description": "Advanced Telegram Intelligence Scraper - Recherche exhaustive et analyse IA pour Telegram",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "test": "jest",
    "lint": "eslint backend/ --ext .js",
    "setup": "node scripts/setup.js",
    "check-apis": "node scripts/check-apis.js",
    "docker:build": "docker build -t telegram-scraper .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "install:frontend": "cd frontend && npm install",
    "build:frontend": "cd frontend && npm run build",
    "clean": "rm -rf node_modules && npm install"
  },
  "keywords": [
    "telegram",
    "scraper",
    "intelligence",
    "osint",
    "api",
    "search",
    "social-media",
    "automation"
  ],
  "author": "Telegram Intelligence Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.0",
    "compromise": "^14.10.0",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^21.11.0",
    "redis": "^4.6.10",
    "sentiment": "^5.0.2",
    "sharp": "^0.32.6",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/telegram-intelligence-scraper.git"
  },
  "bugs": {
